{
  "name": "destek-frontend",
  "version": "0.1.0",
  "private": true,
  "main": "electron-main.js",
  "homepage": "./",
  "scripts": {
    "start": "cross-env HOST=0.0.0.0 PORT=3000 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron electron-main.js",
    "dist": "npm run build && electron-builder"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@microsoft/signalr": "^8.0.7",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "node-fetch": "^2.7.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "electron": "^37.2.1",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.trendteknoloji.destek",
    "productName": "TREND TEKNOLOJÄ° - Destek Sistemi",
    "files": [
      "build/**/*",
      "electron-main.js"
    ],
    "extraResources": [
      {
        "from": "../source/repos/DestekAPI/DestekAPI/bin/Debug/net8.0",
        "to": "backend",
        "filter": [
          "**/*"
        ]
      }
    ],
    "extraMetadata": {
      "main": "electron-main.js"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "mac": {
      "icon": "public/icon.icns"
    },
    "linux": {
      "icon": "public/icon.png"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
